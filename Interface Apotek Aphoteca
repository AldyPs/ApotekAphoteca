    def prosesFile(self):
        self.proses = Apriori.apriorimethod(self.coba, float(self.minSupp.text()), float(self.minConf.text()))
        # print(self.proses)
        self.proses["antecedents"] = [list(x) for x in self.proses["antecedents"]]
        self.proses["consequents"] = [list(x) for x in self.proses["consequents"]]
        self.dataProses = PandasModel(self.proses[['antecedents', 'consequents', 'support', 'confidence', 'lift']])
        self.tabelAsosiasi.setModel(self.dataProses)

    def namaText(self):
        fileName, _ = QtWidgets.QFileDialog.getOpenFileName(None, 'OpenFile', '', 'All Files (*.*)')
        self.namaFile.setText(fileName)

    def bacaFile(self):
        self.coba = pd.read_excel(self.namaFile.text())
        self.df = PandasModel(self.coba)
        self.tabelDataset.setModel(self.df)

    def Save(self):
        name, _ = QtWidgets.QFileDialog.getSaveFileName(None, "Save File", '/', '.csv')
        data = self.proses[['antecedents', 'consequents', 'support', 'confidence', 'lift']]
        data.to_csv(name + ".csv")

